#include <iostream>

using namespace std;


float notas[5];
float soma;
float media;



int main(){
    
    cout << "Escreve as nota dos 5 alunos: " << endl;
    
    for(int i = 0; i<5; i++){ //o numero maximo de alunos adicionando smp mais 1
        cout << "Nota do aluno " << i+1 << ":";
        cin >> notas[i];
        soma += notas[i];
        media = soma /5;
    }
        
    cout << "MÃ©dia: " << media;
    
    
    return 0;
}
